<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
    <defaultCache maxEntriesLocalHeap="20"
                  eternal="false"
                  timeToIdleSeconds="120"
                  timeToLiveSeconds="200"
                  memoryStoreEvictionPolicy="LRU"/>
    <cache name="com.github.ralmnsk.model.news.News"
           maxEntriesLocalHeap="6"
           eternal="true">
    </cache>
    <cache name="com.github.ralmnsk.model.user.User"
           maxEntriesLocalHeap="2"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="600">
        <persistence strategy="localTempSwap"/>
    </cache>
    <cache name="com.github.ralmnsk.model.discussion.Discussion"
           maxEntriesLocalHeap="2"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="600">
        <persistence strategy="localTempSwap"/>
    </cache>
    <cache name="org.hibernate.cache.StandardQueryCache"
           maxEntriesLocalHeap="5"
           eternal="false"
           timeToLiveSeconds="120">
        <persistence strategy="localTempSwap"/>
    </cache>
</ehcache>
